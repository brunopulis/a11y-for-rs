{
  "name": "a11y-for-rs",
  "version": "1.0.0",
  "description": "A11y for RS",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass --watch src/sass -o src/css",
    "watch:eleventy": "eleventy --serve",
    "build:sass": "node-sass src/sass -o src/css",
    "build:eleventy": "eleventy",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:sass --parallel build:*",
    "dev": "npx @11ty/eleventy --serve",
    "clean": "rm -rf build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brunopulis/a11y-for-rs.git"
  },
  "keywords": [
    "Rio",
    "Grande",
    "do",
    "Sul",
    "A11y",
    "doações",
    "donation"
  ],
  "author": "Craig Buckler",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "clean-css": "^5.3.3",
    "html-minifier": "^4.0.0",
    "luxon": "^3.4.4",
    "postcss": "^8.4.38",
    "postcss-advanced-variables": "^4.0.0",
    "postcss-nested": "^6.0.1",
    "postcss-scss": "^4.0.9",
    "rollup-plugin-terser": "^7.0.2"
  },
  "bugs": {
    "url": "https://github.com/brunopulis/a11y-for-rs/issues"
  },
  "homepage": "https://github.com/brunopulis/a11y-for-rs#readme",
  "dependencies": {
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "update": "^0.7.4"
  }
}
